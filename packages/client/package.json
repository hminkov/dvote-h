{
  "name": "@dvote/client",
  "version": "1.0.0",
  "description": "Client library for voting system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "build:prod": "npm run clean && npm run compile:prod",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json --skipLibCheck",
    "compile:prod": "tsc -p tsconfig-prod.build.json --skipLibCheck"
  },
  "dependencies": {
    "@dvote/common": "1.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "private": true
}
